<% include ../head %>

<h1>Upload Dataset File</h1>

<div class="row">
  <div class="col-md-4">
    <form action="/datasets/upload" method="post" role="form" enctype="multipart/form-data">
      <div class="form-group">
        <label for="inputFile">Select File:</label>
        <input type="file" name="file" class="form-control" id="inputFile">
      </div>
      <div class="form-group">
        <label for="inputLocationName">Location Name:</label>
        <input type="text" name="location_name" class="form-control" id="inputLocationName">
      </div>
      <div class="form-group">
        <label for="inputVariableName">Variable Name:</label>
        <input type="text" name="variable_name" class="form-control" id="inputVariableName">
      </div>
      <button type="submit" class="btn btn-default">Submit</button>
    </form>
  </div>
</div>

<% include ../foot %>